with "matreshka_league.gpr";

project Cmof is

   for Object_Dir use ".objs";
   for Source_Dirs use ("cmof", "cmof/.gens", ".");
   for Exec_Dir use ".";
   for Main use ("cmof-main.adb");

   package Compiler is
      for Default_Switches ("Ada") use
        ("-g",
         "-gnat12",
         "-gnata",
         "-gnatn");
   end Compiler;

   package Builder is
      for Executable ("cmof-main.adb") use "demo";
   end Builder;

end Cmof;

      <ownedAttribute xmi:type="cmof:Property" name="symbol">
        <type href="http://www.omg.org/spec/UML/20100901/PrimitiveTypes.cmof#UnlimitedNatural"/>
      </ownedAttribute>
    </ownedType>
    <ownedType xmi:id="EnumLiteralExp" xmi:type="cmof:Class" name="EnumLiteralExp" superClass="LiteralExp">
      <ownedAttribute xmi:type="cmof:Property" name="referredEnumLiteral" lower="0">

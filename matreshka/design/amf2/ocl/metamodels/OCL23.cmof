      <ownedLiteral xmi:type="cmof:EnumerationLiteral" name="OrderedSet"/>
      <ownedLiteral xmi:type="cmof:EnumerationLiteral" name="Bag"/>
      <ownedLiteral xmi:type="cmof:EnumerationLiteral" name="Sequence"/>
    </ownedType>
    <ownedType xmi:id="OclExpression" xmi:type="cmof:Class" isAbstract="true" name="OclExpression">
      <superClass href="http://www.omg.org/spec/UML/20100901/UML.cmof#TypedElement"/>
    </ownedType>

<?xml version="1.0" encoding="utf-8"?>
<!--
  This is modified version of the official OCL 2.3 metamodel. Most significant
  changes are:

   - 'Expressions' and 'Types' nested packages are removed, all elements are in
     one flat package now;

   - references are updated to link to elements of UML 2.4 metamodels
     (PrimitiveTypes and UML).
-->
<xmi:XMI xmi:version="2.1" xmlns:xmi="http://schema.omg.org/spec/XMI/2.1" xmlns:cmof="http://www.omg.org/spec/MOF/200601/cmof">
  <cmof:Package xmi:id="OCL" name="OCL">
    <ownedType xmi:id="BooleanLiteralExp" xmi:type="cmof:Class" name="BooleanLiteralExp" superClass="PrimitiveLiteralExp">
      <ownedAttribute xmi:type="cmof:Property" name="booleanSymbol">
        <type href="http://www.omg.org/spec/UML/20100901/PrimitiveTypes.cmof#Boolean"/>
      </ownedAttribute>
    </ownedType>
    <ownedType xmi:id="CallExp" xmi:type="cmof:Class" isAbstract="true" name="CallExp" superClass="OclExpression">
      <ownedAttribute xmi:type="cmof:Property" isComposite="true" name="source" lower="0" type="OclExpression"/>
    </ownedType>
    <ownedType xmi:id="CollectionKind" xmi:type="cmof:Enumeration" name="CollectionKind">
      <ownedLiteral xmi:type="cmof:EnumerationLiteral" name="Collection"/>

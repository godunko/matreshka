<catalog xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog">

	<!-- Simple suffix tests. -->
	<systemSuffix systemIdSuffix="html1-strict.dtd"
				uri="file:///share/mirrors/w3c/xhtml1/xhtml1-strict.dtd"/>
	
	<uriSuffix uriSuffix="/uniqueName.xsd"
				uri="file:///share/mirrors/schemas/example/uniqueName.xsd"/>

	<!-- Suffix tests for ensuring the longest matches are used. -->

	<systemSuffix systemIdSuffix="match.dtd"
				uri="file:///share/mirrors/ERROR"/>

	<systemSuffix systemIdSuffix="longest-match.dtd"
				uri="http://www.example.org/longestMatch"/>

	<uriSuffix uriSuffix="match.uri"
				uri="http://www.example.org/ERROR"/>

	<uriSuffix uriSuffix="longest-match.uri"
				uri="http://www.example.org/long-uri-match"/>

</catalog>

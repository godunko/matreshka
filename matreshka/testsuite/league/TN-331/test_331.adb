------------------------------------------------------------------------------
--  Checkh whether finalization of JSON_Object and JSON_Value doesn't result in
--  memory corruption.
------------------------------------------------------------------------------
with League.JSON.Objects;
with League.JSON.Values;
with League.Strings;

procedure Test_331 is
   Object : League.JSON.Objects.JSON_Object;
   Value  : League.JSON.Values.JSON_Value;

begin
   Object.Insert
    (League.Strings.To_Universal_String ("name"),
     League.JSON.Values.To_JSON_Value
      (League.Strings.To_Universal_String ("peter")));

   Value := Object.Value (League.Strings.To_Universal_String ("name"));
end Test_331;
